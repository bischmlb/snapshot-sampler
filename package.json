{
    "name": "snapshot-sampler",
    "version": "1.0.0",
    "description": "sends example email from snapshot",
    "main": "index.ts",
    "type": "module",
    "scripts": {
        "build": "npx tsc && tsc-alias",
        "generate:html": "npm run build && node dist/utils/snapshotParser.js",
        "send:all": "npx tsx src/mailer/sendAll.ts",
        "send": "npx tsx src/mailer/send.ts",
        "start": "node dist/index.js"
    },
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@types/express": "^4.17.21",
        "typescript": "^5.5.4"
    },
    "dependencies": {
        "@types/nodemailer": "^6.4.15",
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "nodemailer": "^6.9.14",
        "ts-node": "^10.9.2",
        "tsc-alias": "^1.8.10"
    }
}